{
    "name": "stwarog/uow",
    "description": "Generic Unit of Work pattern implementation, usefull to limit DB queries.",
    "type": "library",
    "require": {
        "webmozart/assert": "1.9.1",
        "myclabs/php-enum": "1.7.7",
        "fuel/orm": "^1.8",
        "fuel/core": "^1.8",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {"Stwarog\\Uow\\": "src/"}
    },
    "autoload-dev": {
        "classmap": [{
            "Tests\\": "tests/"
        }]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Twarog",
            "email": "contact@stwarog.com"
        }
    ],
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["type:fuel-package"]
        },
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "minimum-stability": "stable"
}
